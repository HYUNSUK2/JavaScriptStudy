
<script>

// 화살표함수
  function (x) {  
  retrun x*x;     
  }
  
  = 
  
  (x) =>{x*x}
  
  
  
        //전개연산자
        var sumAll = (...numbers) => {
            var output = 0;
            for( var i = 0; i< numbers.length; i++){
                output += numbers[i];
            }
        }
        
        var array  = [1,2,3,4,5,6,7,];
        
        //ECMAscript5
        sumAll.apply(null,array);
        //ECMAscript6
        sumAll(...array);
  
  
  </script>
  
  
  
  
